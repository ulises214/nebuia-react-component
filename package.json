{
  "name": "nebuia-react-component",
  "description": "Widget to implement Nebuia in React",
  "version": "2.1.6",
  "module": "./dist",
  "types": "./dist",
  "author": "Ulises Rafael Ramirez Martinez",
  "repository": "https://github.com/ulises214/nebuia-react-component.git",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint:fix": "eslint ./src --max-warnings=0 --ext .jsx,.js,.ts,.tsx --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "pnpm lint:format && pnpm lint:fix ",
    "build": "rimraf dist && rollup -c ./config/rollup.config.mjs && pnpm build:tailwind && pnpm build:lazy && pnpm build:clean",
    "build:tailwind": "rollup -c ./config/rollup.tailwind.config.mjs",
    "build:lazy": "rollup -c ./config/rollup.lazy.config.mjs",
    "build:clean": "mv ./dist/src/* ./dist && rm -rf ./dist/src && cp src/base.css dist/base.css && rm -rf ./dist/node_modules",
    "dev": "vite",
    "build:demo": "rimraf demo && vite build --outDir demo",
    "build:demo:debug": "rimraf demo && vite build --mode=demo --outDir demo --base=/nebuia-react-component",
    "deploy:demo": "pnpm build:demo:debug && gh-pages -d demo",
    "upload:demo": "pnpm build:demo && rsync -avz --delete ./demo/* root@194.146.12.166:/root/nebuia_widget_react_2",
    "prepare": "pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/postcss-import": "14.0.3",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/parser": "7.1.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "10.4.17",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-config-codely": "3.1.3",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-tailwindcss": "3.14.3",
    "gh-pages": "6.1.1",
    "postcss": "8.4.35",
    "postcss-import": "16.0.1",
    "rimraf": "5.0.5",
    "rollup": "4.12.0",
    "rollup-plugin-postcss": "4.0.2",
    "tailwindcss": "3.4.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "@tailwindcss/forms": "0.5.7"
  },
  "dependencies": {
    "nebuia-ts": "1.0.4",
    "clsx": "2.1.0",
    "email-validator": "2.0.4",
    "@heroicons/react": "2.1.1",
    "i18next": "23.10.0",
    "lean-qr": "2.3.3",
    "libphonenumber-js": "1.10.57",
    "react": "18.2.0",
    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.5",
    "react-icons": "5.0.1",
    "react-otp-input": "3.1.1",
    "react-phone-input-2": "2.15.1",
    "react-router-dom": "6.22.2",
    "tailwind-merge": "2.2.1",
    "wasm-feature-detect": "1.6.1",
    "zustand": "4.5.1"
  },
  "peerDependencies": {
    "nebuia-ts": "1.0.4",
